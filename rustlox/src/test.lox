
fun fib(n) {
    if (n <= 0) {
        return 0;
    } else if (n == 1) {
        return 1;
    } else {
        return fib(n - 1) + fib(n - 2);
    }
}

for (var i = 0; i < 20; i = i + 1)
    print fib(i);

